<% provide(:title, 'Home') %>
<% if signed_in? %>
  <table class="front">
    <tr>
      <td class="main">
        <h1 class="micropost">What's up?</h1>
        <%= render 'shared/micropost_form' %>
        <%= render 'shared/feed' %>
      </td>
      <td class="sidebar round">
        <%= render 'shared/user_info' %>
        <%= render 'shared/stats' %>
      </td>
    </tr>
  </table>
<% else %>
  <h1>Rent Weird Realty</h1>

  <p>
    Welcome Home
  </p>

  <%= link_to "Sign up now!", signup_path, class: "signup_button round" %>
<br>
<br>
  <%= link_to "Apartment Sign up!", apartment_signup_path, class: "apartment_signup_button round" %>
<% end %>
